{
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "node develop.js",
    "start": "node server.js",
    "format": "prettier --write \"src/*.{js,jsx,json,md,ts,tsx,css}\"",
    "format:check": "prettier --check \"src/*.{js,jsx,json,md,ts,tsx,css}\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --config ./config/eslint.config.js --max-warnings=0 --cache",
    "lint:all": "eslint . --config ./config/eslint.config.js --max-warnings=0 --cache",
    "lint:fix": "eslint src --config ./config/eslint.config.js --max-warnings=0 --fix",
    "precommit": "lint-staged --config ./config/lint-staged.config.json",
    "ci": "npm run typecheck && npm run lint && npm run test:ci && npm run build",
    "test": "vitest --config ./config/vitest.config.ts",
    "test:ci": "node ./scripts/run-tests-inprocess.js",
    "test:run": "vitest run --config ./config/vitest.config.ts",
    "prepare": "husky install"
  },
  "overrides": {
    "three": "^0.180.0"
  },
  "dependencies": {
    "@splinetool/react-spline": "^4.1.0",
    "@splinetool/runtime": "^1.12.29",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "socket.io": "^4.8.3",
    "three": "^0.180.0",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "socket.io-client": "^4.8.3",
    "vite-plugin-bundle-analyzer": "^0.0.1",
    "vitest": "^4.0.15"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.54.0"
  }
}
